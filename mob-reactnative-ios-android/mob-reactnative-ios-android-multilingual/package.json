{
  "name": "snaxgenie",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:reset-cache": "react-native start --reset-cache",
    "start:dev": "npm run start:reset-cache",
    "pretty:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "android": "react-native run-android",
    "android:gradle:permission": "chmod +x gradlew",
    "android:clean": "cd ./android/ && ./gradlew clean && cd ..",
    "android:keystore:debug": "keytool -genkey -v -keystore debug.keystore -alias androiddebugkey -keyalg RSA -keysize 2048 -validity 100000",
    "android:build": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:debug": "npm run android:clean && npm run android:build && android:keystore:debug && ./gradlew assembleDebug && cd ..",
    "android:keystore:release": "keytool -genkey -v -keystore snaxgenie.keystore -alias snaxgenie -keyalg RSA -keysize 2048 -validity 100000",
    "android:release": "npm run android:clean && npm run android:build && android:keystore:release && ./gradlew assembleRelease && cd ..",
    "ios": "react-native run-ios",
    "ios:build": "cd ios && pod install && cd ..",
    "prepare": "husky install"
  },
  "dependencies": {
    "@firebase/app-check-interop-types": "^0.3.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/cli-doctor": "^11.3.8",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-firebase/app": "^18.7.2",
    "@react-native-firebase/database": "^18.7.2",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^20.6.0",
    "react": "18.2.0",
    "react-devtools": "^4.28.0",
    "react-native": "0.72.4",
    "react-native-element-dropdown": "^2.10.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.10.6",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.2",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
